{
  "name": "sole-serachers-frontend",
  "version": "1.0.0",
  "author": "Charlie Waite",
  "scripts": {
    "dev": "next -p 7777",
    "build": "next build",
    "start": "next start",
    "test": "NODE_ENV=test jest --watch",
    "test-win": "SET NODE_ENV=test&& jest --watch",
    "heroku-postbuild": "next build"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.0.1",
    "@apollo/react-ssr": "3.0.0",
    "apollo-boost": "^0.4.4",
    "apollo-client": "^2.6.4",
    "apollo-link-context": "^1.0.18",
    "babel-core": "^6.26.3",
    "babel-plugin-styled-components": "^1.10.6",
    "date-fns": "^2.0.0",
    "downshift": "^3.2.12",
    "graphql": "^14.5.0",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "jest-styled-components": "^6.3.3",
    "lodash.debounce": "^4.0.8",
    "next": "^9.0.4",
    "next-with-apollo": "^4.2.0",
    "nprogress": "^0.2.0",
    "pluralize": "^8.0.0",
    "react": "^16.9.0",
    "react-adopt": "^0.6.0",
    "react-apollo": "^3.0.1",
    "react-content-loader": "^4.2.2",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.9.0",
    "react-stripe-checkout": "^2.6.3",
    "react-transition-group": "^4.2.2",
    "styled-components": "^4.3.2",
    "typescript": "^3.5.3",
    "use-media-hook": "^1.0.4",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "casual": "^1.6.2",
    "graphql-tools": "^4.0.5",
    "jest": "^24.9.0",
    "jest-transform-graphql": "^2.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest",
      "^.+\\.js?$": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ],
          "inline-react-svg"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ],
          "inline-react-svg"
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
